<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@../stylesheets/Stylesheet1.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.mvwojcik.controllers.CategoryController">
   <center>
      <VBox prefHeight="500.0" prefWidth="400.0" spacing="40.0">
         <children>
            <Label alignment="TOP_CENTER" prefHeight="600.0" prefWidth="900.0" text="Ideas for...">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Label>
            <Button mnemonicParsing="false" onAction="#breakfastOnAction" prefHeight="200.0" prefWidth="900.0" text="Meat">
               <VBox.margin>
                  <Insets left="60.0" right="70.0" top="15.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#dinnerOnAction" prefHeight="200.0" prefWidth="900.0" text="Carbos">
               <VBox.margin>
                  <Insets left="60.0" right="70.0" />
               </VBox.margin>
            </Button>
            <Button alignment="TOP_CENTER" mnemonicParsing="false" onAction="#supperOnAction" prefHeight="200.0" prefWidth="900.0" text="Fats">
               <VBox.margin>
                  <Insets left="60.0" right="70.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#snacksOnAction" prefHeight="200.0" prefWidth="900.0" text="Snacks">
               <VBox.margin>
                  <Insets left="60.0" right="70.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#returnOnAction" prefHeight="200.0" prefWidth="900.0" text="Return">
               <VBox.margin>
                  <Insets bottom="40.0" left="60.0" right="70.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </center>
</BorderPane>
