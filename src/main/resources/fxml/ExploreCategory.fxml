<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.mvwojcik.controllers.CategoryController">
   <center>
      <VBox prefHeight="500.0" prefWidth="400.0" spacing="30.0" stylesheets="@../stylesheets/MainSceneStylesheet0.css">
         <children>
            <Label alignment="TOP_CENTER" prefHeight="18.0" prefWidth="358.0" text="Ideas for..." />
            <Button mnemonicParsing="false" onAction="#breakfastOnAction" prefHeight="32.0" prefWidth="202.0" text="Breakfast">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#dinnerOnAction" prefHeight="39.0" prefWidth="206.0" text="dinner">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
            <Button alignment="TOP_CENTER" mnemonicParsing="false" onAction="#supperOnAction" prefHeight="39.0" prefWidth="206.0" text="supper">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#snacksOnAction" prefHeight="39.0" prefWidth="210.0" text="Snacks">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#returnOnAction" prefHeight="33.0" prefWidth="218.0" text="Return">
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </center>
</BorderPane>
